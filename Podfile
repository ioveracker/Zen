source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!

target 'iOS' do
  pod 'Moya', '~> 6.4.0'

  target 'iOSTests' do
    inherit! :search_paths
    pod 'Quick', '~> 0.9.2'
    pod 'Nimble', '~> 4.0.0'
  end
end

target 'tvOS' do
  pod 'Moya', '~> 6.4.0'

  target 'tvOSTests' do
    inherit! :search_paths
    pod 'Quick', '~> 0.9.2'
    pod 'Nimble', '~> 4.0.0'
  end
end

